+++ linux-2.6.36-dev/mycall/mycall.c	2018-09-08 00:05:44.000000000 -0400
@@ -0,0 +1,20 @@
+/*---Start of mycall.c----*/
+#include<linux/linkage.h>
+#include<linux/time.h>
+#include<linux/sched.h>
+
+asmlinkage long sys_jasmine_batten(int i)
+{
+	struct timeval t;
+	struct tm b;
+
+	printk("sys_jasmine_batten called from process %d with panther ID %d.\n", current->pid, i);
+
+	do_gettimeofday(&t);
+	time_to_tm(t.tv_sec, 0, &b);
+	printk("%02d:%02d:%02d\n", b.tm_hour - 4, b.tm_min, b.tm_sec);
+
+	return 0;
+}
+
+/*---End of mycall.c-----*/
