+++ linux-2.6.36-dev/mycall/mycall.c	2018-09-11 15:49:48.000000000 -0400
@@ -0,0 +1,27 @@
+#include<linux/linkage.h>
+#include<linux/kernel.h>
+#include<linux/time.h>
+#include<linux/sched.h>
+
+asmlinkage long sys_jose_toledo (int pantherid) {
+
+struct timespec ts;
+struct tm tv;
+unsigned long get_time;
+
+printk ("sys_jose_toledo called from process %d with panther ID %d.\n", current->pid, pantherid);
+
+getnstimeofday(&ts);
+get_time = ts.tv_sec;
+time_to_tm(get_time, 0, &tv);
+
+printk("Time: %.2lu:%.2lu:%.2lu ",
+		((ts.tv_sec/3600)%24)-4,
+		(ts.tv_sec / 60)% 60,
+		(ts.tv_sec %60));
+
+printk("Day: %d Month: %d Year: %d", tv.tm_mday ,(tv.tm_mon + 1), (tv.tm_year + 1900));
+
+
+return 0;
+}
